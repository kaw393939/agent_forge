# Agent Forge Helm Chart Secrets
# Copy this file to secrets.yaml and fill in your actual secrets
# IMPORTANT: Never commit the actual secrets.yaml file to version control

secrets:
  # Docker Hub authentication token
  dockerHubToken: "your-docker-hub-token"
  
  # OpenAI API key for agents
  openaiApiKey: "your-openai-api-key"

  # DigitalOcean API token for Terraform
  digitalOceanToken: "your-digitalocean-token"
